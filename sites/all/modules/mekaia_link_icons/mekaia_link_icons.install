<?php

/**
 * @file
 * Link icons install file.
 */

/**
 * Implements hook_uninstall().
 */
function mekaia_link_icons_uninstall() {
  // Delete variables.
  db_delete('variable')
    ->condition('name', db_like('mekaia_link_icon') . '%', 'LIKE')
    ->execute();

  // Clear the variable cache.
  cache_clear_all('variables', 'cache_bootstrap');
}
